{
    "name": "velocity-artisans/nova-featured-products",
    "description": "A Laravel Nova tool for featured products for Velocity Artisans.",
    "keywords": [
        "laravel",
        "nova",
        "velocity artisans"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "VelocityArtisans\\FeaturedProducts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VelocityArtisans\\FeaturedProducts\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
